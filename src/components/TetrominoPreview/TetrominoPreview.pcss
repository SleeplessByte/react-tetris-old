.preview {
  table-layout: fixed;
  border-collapse: collapse;
  width: calc(4 * 36px);
  height: calc(4 * 36px);

  margin-left: 16px;

  & tr {
    width: 100%;
    height: 36px;
  }

  & td {
    border: 1px solid #EEEEEE;
    width: 36px;
    height: 36px;
    overflow: hidden;

    font-size: 10px;
  }
}
